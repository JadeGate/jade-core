{
  "jade_version": "1.0.0",
  "skill_id": "mcp_replicate_predict",
  "metadata": {
    "name": "MCP Replicate Prediction",
    "version": "1.0.0",
    "description": "Run AI model predictions on Replicate.",
    "author": "jade-mcp-bridge",
    "tags": [
      "mcp",
      "replicate",
      "ai",
      "inference"
    ],
    "license": "BSL-1.1",
    "created_at": "2026-02-22T16:23:40Z",
    "updated_at": "2026-02-22T16:23:40Z"
  },
  "trigger": {
    "type": "task_intent",
    "conditions": [
      {
        "field": "task.type",
        "operator": "in",
        "value": [
          "mcp_replicate_predict"
        ]
      }
    ]
  },
  "input_schema": {
    "required_params": [
      {
        "name": "model",
        "type": "string",
        "description": "Model version ID"
      },
      {
        "name": "input",
        "type": "object",
        "description": "Model input parameters"
      }
    ],
    "optional_params": []
  },
  "output_schema": {
    "fields": [
      {
        "name": "output",
        "type": "object",
        "description": "Prediction output"
      },
      {
        "name": "status",
        "type": "string",
        "description": "Status"
      }
    ]
  },
  "execution_dag": {
    "nodes": [
      {
        "id": "predict",
        "action": "http_post",
        "params": {
          "url": "https://api.replicate.com/v1/predictions",
          "headers": {
            "Authorization": "Bearer {{env.REPLICATE_TOKEN}}",
            "Content-Type": "application/json"
          },
          "body": {
            "version": "{{input.model}}",
            "input": "{{input.input}}"
          }
        }
      },
      {
        "id": "chk",
        "action": "condition_check",
        "params": {
          "field": "{{predict.output.status_code}}",
          "operator": "equals",
          "value": 201
        }
      },
      {
        "id": "ext",
        "action": "json_extract",
        "params": {
          "data": "{{predict.output.body}}",
          "path": "$",
          "fields": [
            "id",
            "status",
            "output"
          ]
        }
      },
      {
        "id": "ret",
        "action": "return_result",
        "params": {
          "output": "{{ext.output.value}}",
          "status": "ok"
        }
      }
    ],
    "edges": [
      {
        "from": "predict",
        "to": "chk"
      },
      {
        "from": "chk",
        "to": "ext"
      },
      {
        "from": "ext",
        "to": "ret"
      }
    ],
    "entry_node": "predict",
    "exit_node": [
      "ret"
    ]
  },
  "security": {
    "network_whitelist": [
      "api.replicate.com"
    ],
    "file_permissions": {
      "read": [],
      "write": []
    },
    "max_execution_time_ms": 15000,
    "max_retries": 1,
    "sandbox_level": "standard",
    "dangerous_patterns": [],
    "env_whitelist": [
      "REPLICATE_TOKEN"
    ]
  },
  "mcp_compatible": true,
  "required_mcp_capabilities": [
    "replicate"
  ],
  "jade_signature": {
    "version": "1.0.0",
    "skill_id": "mcp_replicate_predict",
    "content_hash": "sha256:f16216aa8119a718c00059b46aa92850e13df8aab5138923258c71ff988bdbd6",
